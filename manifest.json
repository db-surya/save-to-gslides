{
  "name": "Save to Google Slides",
  "version": "1.0",
  "manifest_version": 3,
  "description": "This is to save the images/text to google slides",
  "action": {
    "default_popup": "login.html"
  },
  "permissions": [
    "identity",
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "key": "",
  "background": {
    "service_worker": "background.js"
  }
}